{
    "name": "TikTok Scraper Extension",
    "description": "Chrome Extension to scrape content off of TikTok!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}